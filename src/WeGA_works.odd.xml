                    <elementSpec ident="source" module="MEI.header" mode="change"
                        ns="http://www.music-encoding.org/ns/mei">
                        <content>
                            <rng:zeroOrMore>
                                <rng:ref name="model.identifierLike"/>
                            </rng:zeroOrMore>
                            <rng:optional>
                                <rng:ref name="titleStmt"/>
                            </rng:optional>
                            <rng:ref name="macro.bibldescPart"/>
                            <rng:optional>
                                <rng:ref name="incip"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="physDesc"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="physLoc"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="contents"/>
                            </rng:optional>
                            <!-- Adding history to source -->
                            <rng:optional>
                                <rng:ref name="history"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="langUsage"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="notesStmt"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="classification"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="itemList"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="componentGrp"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="componentGrp"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="relationList"/>
                            </rng:optional>
                        </content>
                        <constraintSpec scheme="isoschematron" ident="id-check">
                            <constraint>
                                <sch:rule context="mei:source">
                                    <sch:assert test="@xml:id or ancestor::mei:source"> Am
                                        Wurzelelement muss eine XML-ID angegeben sein. </sch:assert>
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                        <attList>
                            <attDef ident="xml:id" mode="change">
                                <datatype>
                                    <rng:ref name="key.sources.pattern"/>
                                </datatype>
                            </attDef>
                            <attDef ident="type" mode="change" usage="opt">
                                <valList mode="replace" type="semi">
                                    <valItem ident="ms">
                                        <desc>Manuscript</desc>
                                    </valItem>
                                    <valItem ident="ms_score">
                                        <desc>Score in manuscript form</desc>
                                    </valItem>
                                    <valItem ident="ms_piano_score">
                                        <desc>Piano score in manuscript form</desc>
                                    </valItem>
                                    <valItem ident="ms_vocal_score">
                                        <desc>Vocal score in manuscript form</desc>
                                    </valItem>
                                    <valItem ident="ms_solo_part">
                                        <desc>Solo part in manuscript form</desc>
                                    </valItem>
                                    <valItem ident="ms_choral_part">
                                        <desc>Choral part in manuscript form</desc>
                                    </valItem>
                                    <valItem ident="ms_instrumental_part">
                                        <desc>Instrumental part in manuscript form</desc>
                                    </valItem>
                                    <valItem ident="ms_script">
                                        <desc>Handwritten script</desc>
                                    </valItem>
                                    <valItem ident="ms_prompt_book">
                                        <desc>Handwritten prompt book</desc>
                                    </valItem>
                                    <valItem ident="ms_textbook">
                                        <desc>Handwritten textbook</desc>
                                    </valItem>
                                    <valItem ident="ms_inner_sheet">
                                        <desc>Handwritten inner sheet</desc>
                                    </valItem>
                                    <valItem ident="ms_cut_sheet">
                                        <desc>Handwritten cut sheet</desc>
                                    </valItem>
                                    <valItem ident="pr">
                                        <desc>Print</desc>
                                    </valItem>
                                    <valItem ident="pr_score">
                                        <desc>Printed score</desc>
                                    </valItem>
                                    <valItem ident="pr_piano_score">
                                        <desc>Printed piano score</desc>
                                    </valItem>
                                    <valItem ident="pr_vocal_score">
                                        <desc>Printed vocal score</desc>
                                    </valItem>
                                    <valItem ident="pr_solo_part">
                                        <desc>Printed solo part</desc>
                                    </valItem>
                                    <valItem ident="pr_choral_part">
                                        <desc>Printed choral part</desc>
                                    </valItem>
                                    <valItem ident="pr_instrumental_part">
                                        <desc>Printed instrumental part</desc>
                                    </valItem>
                                    <valItem ident="pr_script">
                                        <desc>Printed script</desc>
                                    </valItem>
                                    <valItem ident="pr_prompt_book">
                                        <desc>Printed prompt book</desc>
                                    </valItem>
                                    <valItem ident="pr_textbook">
                                        <desc>Printed textbook</desc>
                                    </valItem>
                                    <valItem ident="pr_inner_sheet">
                                        <desc>Printed inner sheet</desc>
                                    </valItem>
                                    <valItem ident="pr_cut_sheet">
                                        <desc>Printed cut sheet</desc>
                                    </valItem>
                                </valList>
                            </attDef>
                            <attDef ident="event" mode="change">
                                <datatype>
                                    <rng:ref name="key.sources.pattern"/>
                                </datatype>
                            </attDef>
                        </attList>
                    </elementSpec>
                    
                    <elementSpec ident="work" module="MEI.header" mode="change"
                        ns="http://www.music-encoding.org/ns/mei">
                        <content>
                            <rng:zeroOrMore>
                                <rng:ref name="model.identifierLike"/>
                            </rng:zeroOrMore>
                            <rng:zeroOrMore>
                                <rng:ref name="title"/>
                            </rng:zeroOrMore>
                            <rng:zeroOrMore>
                                <rng:ref name="author"/>
                            </rng:zeroOrMore>
                            <rng:zeroOrMore>
                                <rng:ref name="composer"/>
                            </rng:zeroOrMore>
                            <rng:zeroOrMore>
                                <rng:ref name="librettist"/>
                            </rng:zeroOrMore>
                            <rng:zeroOrMore>
                                <rng:ref name="lyricist"/>
                            </rng:zeroOrMore>
                            <rng:zeroOrMore>
                                <rng:ref name="arranger"/>
                            </rng:zeroOrMore>
                            <rng:zeroOrMore>
                                <rng:ref name="contributor"/>
                            </rng:zeroOrMore>
                            <rng:optional>
                                <rng:ref name="incip"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="creation"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="history"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="langUsage"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="perfMedium"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="contents"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="notesStmt"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="classification"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="bibl"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="itemList"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="expressionList"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="componentList"/>
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="relationList"/>
                            </rng:optional>
                        </content>
                        <constraintSpec scheme="isoschematron" ident="id-check">
                            <constraint>
                                <sch:rule context="mei:source">
                                    <sch:assert test="@xml:id or ancestor::mei:source"> Am
                                        Wurzelelement muss eine XML-ID angegeben sein. </sch:assert>
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                    </elementSpec>

                    <macroSpec ident="data.ISODATE" module="MEI" mode="change">
                        <content>
                            <rng:ref name="wega.data.temporal.iso"/>
                        </content>
                    </macroSpec>

                    <elementSpec ident="date" module="MEI.shared" mode="change"
                        ns="http://www.music-encoding.org/ns/mei">
                        <classes mode="change">
                            <memberOf key="att.common" mode="delete"/>
                            <memberOf key="att.facsimile" mode="delete"/>
                            <memberOf key="att.lang" mode="delete"/>
                        </classes>
                    </elementSpec>

                    <elementSpec ident="altId" module="MEI.header" mode="change"
                        ns="http://www.music-encoding.org/ns/mei">
                        <classes mode="change">
                            <memberOf key="att.bibl" mode="delete"/>
                        </classes>
                        <content>
                            <rng:text/>
                        </content>
                        <attList>
                            <attDef ident="xml:base" mode="delete"/>
                            <attDef ident="type" mode="change" usage="req">
                                <valList mode="replace" type="semi">
                                    <valItem ident="gnd">
                                        <gloss versionDate="2014-04-30" xml:lang="de">Gemeinsame
                                            Normdatei</gloss>
                                        <gloss versionDate="2014-04-30" xml:lang="en">Gemeinsame
                                            Normdatei</gloss>
                                        <desc versionDate="2014-04-30" xml:lang="de">Markiert
                                            Angaben (ID sowie Einheitssachtitel) nach der
                                            Gemeinsamen Normdatei.</desc>
                                        <desc versionDate="2014-04-30" xml:lang="en">marks
                                            information (ID and title) from the German authority
                                            file "Gemeinsamen Normdatei".</desc>
                                    </valItem>
                                    <valItem ident="opus">
                                        <desc versionDate="2014-04-30" xml:lang="de">Markiert eine
                                            Opuszahl.</desc>
                                        <desc versionDate="2014-04-30" xml:lang="en">marks an opus
                                            number.</desc>
                                    </valItem>
                                    <valItem ident="WeGA">
                                        <gloss versionDate="2014-04-30" xml:lang="de">Werke</gloss>
                                        <gloss versionDate="2014-04-30" xml:lang="en">Werke</gloss>
                                        <desc versionDate="2014-04-30" xml:lang="de">Markiert eine
                                            ID aus der WeGA</desc>
                                        <desc versionDate="2014-04-30" xml:lang="en">ID aus der
                                            WeGA</desc>
                                    </valItem>
                                </valList>
                            </attDef>
                        </attList>
                    </elementSpec>

                    <elementSpec ident="titleStmt" module="MEI.header" mode="change"
                        ns="http://www.music-encoding.org/ns/mei">
                        <content>
                            <rng:oneOrMore>
                                <rng:ref name="title"/>
                            </rng:oneOrMore>
                            <rng:optional>
                                <rng:ref name="respStmt"/>
                            </rng:optional>
                        </content>
                        <attList>
                            <attDef ident="label" mode="delete"/>
                            <attDef ident="xml:base" mode="delete"/>
                            <attDef ident="n" mode="delete"/>
                        </attList>
                    </elementSpec>

                    <elementSpec ident="title" module="MEI.shared" mode="change" ns="http://www.music-encoding.org/ns/mei">
                        <classes mode="change">
                            <memberOf key="att.authorized" mode="delete"/>
                            <memberOf key="att.canonical" mode="delete"/>
                            <memberOf key="att.facsimile" mode="delete"/>
                            <memberOf key="att.edit" mode="add"/>
                        </classes>
                        <content>
                            <rng:oneOrMore>
                                <rng:choice>
                                    <rng:text/>
                                    <rng:ref name="lb"/>
                                    <rng:ref name="persName"/>
                                    <rng:ref name="name"/>
