        </sch:rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="wegaWorks-dynam-dynam_end-type_attributes-constraint-rule-49">
        <sch:rule xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:s="http://www.ascc.net/xml/schematron" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" context="mei:dynam[@val2]">
