    >> sch:pattern [
         id =
           "wegaWorks-incipCode-Check_incipCode_form_mimetype-constraint-rule-72"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "mei:incipCode"
           "\x{a}" ~
